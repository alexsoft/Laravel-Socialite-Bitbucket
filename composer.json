{
    "name": "alexsoft/laravel-socialite-bitbucket",
    "description": "Bitbucket OAuth2 Provider for Laravel Socialite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "laravel/socialite": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Alexsoft\\LaravelSocialiteBitbucket\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alexsoft\\LaravelSocialiteBitbucket\\Provider"
            ]
        }
    }
}
